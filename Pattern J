import java.util.Scanner;
public class pattern1
{
	public static void main(String[] args){
		Scanner scanner = new Scanner(System.in);
		System.out.print("The pattern is :\n");
		for (int j=1;j<=5 ;j++ )
		{
			for (int i=1;i<=j ;i++ )
			{
				System.out.print(i);
			}
			System.out.println();
		}
}
}
