import java.util.*;
public class Commonnum {
    public static void main(String[] args) {
        int[] a = {4,5,8,90,76,};
        int[] b = {12,2,90,7};
        List<Integer> common = new ArrayList<>();
        for (int i : a) {
            for (int j : b) {
                if (i == j) {
                    common.add(i);
                }
				
            }
        }
        System.out.println("Common elements: " + common);
    }
}
